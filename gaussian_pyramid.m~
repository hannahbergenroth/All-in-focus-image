function g_pyr = gaussian_pyramid(I,level)

% I - input image
% level - number of levels in pyramid

% allocate pyramid of n levels
g_pyr = cell( level, 1 );

% save input image in first level at the pyramid
pyr_g{1} = 1


for n=2:level

    % Lowpass filter
    I_filtered = imgaussfilt(I,2);
    J = imresize(I,scale)

end



end

